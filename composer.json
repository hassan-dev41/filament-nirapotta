{
    "name": "frentors/filament-nirapotta",
    "description": "A role and permission management system for Filament Admin Panel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Frentors\\FilamentNirapotta\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Frentors",
            "email": "support@frentors.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "spatie/laravel-permission": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Frentors\\FilamentNirapotta\\Providers\\FilamentNirapottaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}