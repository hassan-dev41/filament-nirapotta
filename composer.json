{
    "name": "hassan-dev41/filament-nirapotta",
    "description": "Role and permission management for FilamentPHP using Spatie Permission",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HassanDev41\\FilamentNirapotta\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohammad Hassan",
            "email": "devhassan41@gmail.com"
        }
    ],
